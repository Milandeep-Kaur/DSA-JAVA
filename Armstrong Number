public class Main
{
	public static void main(String[] args) {
		
		int number=371;
		
		int result=0;
		int rem;
		int originalnum=number;
		while(originalnum!=0)
		{
		    rem=originalnum%10;
		    result+=Math.pow(rem,3);
		    originalnum=originalnum/10;
		    
		}
		if(number==result)
		{
		    System.out.println(number+" is an Armstrong no.");
		}
		else{
		    System.out.println(number+" is not an Armstrong no.");
		}
	}
}
