public class Main {
    public static void main(String[] args) {
        
       int a=20;
       int b=30;
       int temp;
       System.out.println("Original value of a="+ a +" ,b="+b);
       temp=a;
       a=b;
       b=temp;
       
       System.out.println("After swapping value of a="+a+" ,b="+b);
    }
}
